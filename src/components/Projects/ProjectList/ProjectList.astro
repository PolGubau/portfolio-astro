---
import ProjectInList from "@c/Projects/ProjectInList/ProjectInList.astro";
import { type Project } from "@/types.d";
import projects from "../../../data/projects.json";

const allProjects = projects as Project[];
---

<ul
  id="projectList"
  class="gap-8 px-8 pb-8 max-w-5xl grid grid-cols-1 md:grid-cols-2 w-full"
>
  {allProjects.map((p) => <ProjectInList project={p} />)}
</ul>
